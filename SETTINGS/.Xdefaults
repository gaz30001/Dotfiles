URxvt.letterSpace: 1
URxvt.intensityStyles:	false
urxvt.background:       [70]#575757
URxvt.depth:		30
URxvt.font: 	xft:monospace:size=10, xft:FontAwesome:size=9, xft:Icons:size=9, xft:Powerline:normal:size=10
!!URxvt.font: xft:Inconsolata\ for\ Powerline:pixelsize=12:antialias=true:hinting=true

!!URxvt.font: xft:monospace:size=10
URxvt*termName: rxvt-unicode-256color
URxvt.scrollBar:	false
URxvt.cursorColor:	#b7ce42
!!URxvt.font: xft:Inconsolata\ for\ Powerline:pixelsize=12
!! Extensions
URxvt.perl-ext-common:	default,matcher,resize-font,url-select,keyboard-select
URxvt.colorUL:		#4682B4
!! url-select
URxvt.keysym.M-u:	perl:url-select:select_next
URxvt.url-select.launcher: webview
URxvt.url-select.underline: true
!! keyboard-select:
URxvt.keysym.M-Escape: perl:keyboard-select:activate
!! resize-font
URxvt.resize-font.smaller:	C-Down
URxvt.resize-font.bigger:	C-Up
!! Matcher
URxvt.url-launcher:	webview
URxvt.matcher.button:	1

rofi.color-enabled:	true
rofi.color-window:	#000, #000, #000
rofi.color-normal:	#111, #819396, #222, #008ed4, #ffffff
rofi.color-active:	#002b37, #008ed4, #003643, #008ed4, #66c6ff
rofi.color-urgent:	#002b37, #da4281, #003643, #008ed4, #890661

rofi.fake-transparency:	true
rofi.lines:		3
rofi.bw:		0
rofi.opacity:		"10"
rofi.hide-scrollbar:	false
rofi.width:		30

!XTerm.vt100.background: #232324
XTerm.termName: xterm-256color
XTerm.vt100.locale: true
XTerm.vt100.reverseVideo: true
XTerm.vt100.faceName: xft:monospace:size=10
XTerm.vt100.font: 7x13
